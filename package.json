{
  "name": "analytics",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "start:aot": "ng serve --prod",
    "build:aot": "ng build --prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "precommit": "lint-staged",
    "bootstrap": "lerna bootstrap",
    "postbuild": "rimraf dist/**/*.ngsummary.json dist/**/src/*.ngsummary.json",
    "clean": "git clean -xdf && yarn && yarn run bootstrap",
    "cli": "ng",
    "ci": "yarn run build && yarn run test",
    "prettier": "prettier --parser typescript --single-quote --trailing-comma es5 --write \"**/*.ts\"",
    "postinstall": "opencollective postinstall",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "release": "lerna publish --skip-npm --conventional-commits && npm run build",
    "snapshot": "jest --updateSnapshot"
  },
  "private": true,
  "engines": {
    "node": ">=8.6.0",
    "npm": ">=5.3.0",
    "yarn": ">=1.3.0 <2.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "yarn prettier",
      "git add"
    ]
  },
  "keywords": [
    "ngrx",
    "angular",
    "rxjs"
  ],
  "repository": {},
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/cdk": "^5.0.1",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/platform-server": "^5.0.0",
    "@angular/router": "^5.0.0",
    "core-js": "^2.4.1",
    "opencollective": "^1.0.3",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.6.0",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@angular/material": "5.0.1",
    "@ngrx/db": "^2.1.0",
    "@ngrx/effects": "^4.1.1",
    "@ngrx/entity": "^4.1.1",
    "@ngrx/router-store": "^4.1.1",
    "@ngrx/store": "^4.1.1",
    "@ngrx/store-devtools": "^4.1.1",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/jest": "^20.0.2",
    "@types/lodash": "^4.14.80",
    "@types/node": "~6.0.60",
    "@types/rimraf": "^0.0.28",
    "codelyzer": "^4.0.1",
    "conventional-changelog": "^1.1.4",
    "deep-freeze": "^0.0.1",
    "hammerjs": "^2.0.8",
    "jasmine": "^2.5.3",
    "jasmine-core": "~2.6.2",
    "jasmine-marbles": "^0.0.2",
    "jasmine-spec-reporter": "~4.1.0",
    "jest": "^21.0.2",
    "jest-preset-angular": "^3.0.1",
    "jest-zone-patch": "^0.0.7",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lerna": "^2.5.1",
    "lint-staged": "^4.0.3",
    "ngrx-store-freeze": "^0.2.0",
    "prettier": "^1.5.2",
    "protractor": "~5.1.2",
    "reflect-metadata": "^0.1.9",
    "rimraf": "^2.5.4",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/setup-jest.ts"
  }
}
